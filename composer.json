{
    "name": "sonata-project/core-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataCoreBundle (abandoned)",
    "keywords": [
        "sonata"
    ],
    "homepage": "https://sonata-project.org/bundles/core",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataCoreBundle/contributors"
        }
    ],
    "require": {
        "php": "8.1",
        "cocur/slugify": "^3.0 || v4.3.0",
        "sonata-project/doctrine-extensions": "^1.5",
        "sonata-project/form-extensions": "^0.1.1",
        "sonata-project/twig-extensions": "^0.1.1",
        "symfony/config": "v6.2.7",
        "symfony/dependency-injection": "v6.1.12",
        "symfony/event-dispatcher": "v6.2.7",
        "symfony/form": "v6.2.7",
        "symfony/framework-bundle": "v6.2.7",
        "symfony/http-foundation": "v6.2.7",
        "symfony/http-kernel": "v6.2.7",
        "symfony/options-resolver": "v6.2.7",
        "symfony/property-access": "v6.1.3",
        "symfony/security-csrf": "v6.2.0",
        "symfony/translation": "^v6.1.11",
        "symfony/twig-bridge": "^v6.0.16",
        "symfony/validator": "v6.2.7",
        "twig/extensions": "^1.5",
        "twig/twig": "^2.12.1"
    },
    "conflict": {
        "sonata-project/exporter": "< 1.11"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "doctrine/phpcr-odm": "^1.0",
        "jackalope/jackalope-doctrine-dbal": "^1.0",
        "jms/serializer-bundle": "^2.1 || ^3.0",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "nelmio/api-doc-bundle": "^2.11",
        "phpspec/prophecy": "^1.10.3",
        "sonata-project/exporter": "^1.11 || ^2.0",
        "symfony/browser-kit": "^3.4 || ^4.3 || ^5.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "suggest": {
        "sonata-project/doctrine-extensions": "Direct replacement for Doctrine behavioral extensions",
        "sonata-project/form-extensions": "Direct replacement for Symfony form extensions",
        "sonata-project/twig-extensions": "Direct replacement for Twig Extensions"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sonata\\CoreBundle\\": "src/CoreBundle/",
            "Sonata\\Serializer\\": "src/Serializer/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\CoreBundle\\Tests\\": "tests/CoreBundle/",
            "Sonata\\Serializer\\Tests\\": "tests/Serializer/"
        }
    },
    "abandoned": true
}
